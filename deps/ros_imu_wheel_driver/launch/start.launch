<launch>
  <arg name="FrameId" default="base_link" />
  <arg name="R" default="0.07" />
  <arg name="B" default="0.24" />
  <arg name="freq" default="20" />
  <arg name="rate" default="115200" /> 
  <arg name="name" default="/dev/ttyAMA4" />
  <arg name="imu_topic" default="/imu0/data_raw" />
   <arg name="imu_one_topic" default="/imu1/data_raw" />
  <arg name="wheel_topic" default="/xiaomi/wheel" />
  <arg name="crash_topic" default="/xiaomi/crash_sensor" />
  <arg name="camera_sub_topic" default="image" />
  <arg name="camera_compressed_sub_topic" default="image/compressed" />
  <arg name="camera_pub_topic" default="/raspicam_node/camera_image" />
    <arg name="camera_compressed_pub_topic" default="/raspicam_node/camera_compressed_image" />
  <arg name="output_path" default="/home/pi/Documents/output.txt" />

  <node name="ros_imu_wheel_driver" pkg="ros_imu_wheel_driver" type="ros_imu_wheel_driver_node" output="screen">
    <param name="port" value="$(arg name)" />
    <param name="speed" value="$(arg rate)" />
    <param name="frequency" value="$(arg freq)" />
    <param name="radius" value="$(arg R)" />
    <param name="base_line" value="$(arg B)" />
    <param name="imu_topic" value="$(arg imu_topic)" />
      <param name="imu_one_topic" value="$(arg imu_one_topic)" />
    <param name="odom_topic" value="$(arg wheel_topic)" />
    <param name="crash_topic" value="$(arg crash_topic)" />
    <param name="camera_sub_topic" value="$(arg camera_sub_topic)" />
      <param name="camera_compressed_sub_topic" value="$(arg camera_compressed_sub_topic)" />
      <param name="camera_pub_topic" value="$(arg camera_pub_topic)" />
       <param name="camera_compressed_pub_topic" value="$(arg camera_compressed_pub_topic)" />
    <param name="frame_id" value="$(arg FrameId)" />
    <param name="dump" value="false" />
    <param name="output" value="$(arg output_path)" />  
  </node>
</launch>

